{
  "name": "dwncrwlr",
  "version": "0.1.0",
  "description": "Static site generator which generates EJS suppored HTML files from Markdown, HTML and EJS input.",
  "main": "dwncrwlr.js",
  "bin": {
    "crwldwn": "./dwncrwlr.js"
  },
  "scripts": {
    "dev:start": "node dist/dwncrwlr.js dwncrwlr.config.json",
    "build": "rm -rf dist/ && gulp && npm run clean-package",
    "clean-package": "node ./buildtools/cleanup-package.js",
    "build:dev": "npm run tsc && npm run styles && npm run generate:ts",
    "styles": "node-sass ./profile/styles/scss/index.scss ./profile/assets/css/styles.css",
    "generate:ts": "node ./src/dwncrwlr.js",
    "watch": "watch 'npm run build' ./src",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "serve": "serve ./publish"
  },
  "keywords": [],
  "author": "Christian Silfang (comcy)",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.9",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "fs": "0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jasmine": "^3.3.1",
    "jasmine-core": "^3.3.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-trx-reporter": "^2.2.2",
    "jasmine-ts": "^0.3.0",
    "jest": "^29.5.0",
    "jest-html-reporter": "^3.10.0",
    "nlf": "^1.4.0",
    "node-sass": "^9.0.0",
    "serve": "^14.2.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslint": "^5.11.0",
    "typescript": "^5.0.4",
    "watch": "^0.13.0"
  },
  "dependencies": {
    "ejs": "^3.1.9",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.6",
    "moment": "^2.29.4",
    "npm": "^9.6.7",
    "showdown": "^2.1.0"
  },
  "repository": "https://github.com/comcy/DWNCRWLR.git"
}
